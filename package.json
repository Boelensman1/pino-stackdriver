{
  "name": "pino-stackdriver",
  "version": "1.0.0-alpha",
  "description": "A transport for pino that sends messages to Stackdriver Logging",
  "homepage": "https://github.com/ovhemert/pino-stackdriver",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "standard && tap test/*.test.js --coverage --100",
    "test:appveyor": "standard",
    "test:report": "standard && tap test/*.test.js --coverage  --coverage-report=html --100",
    "posttest": "tap --coverage --coverage-report=lcovonly"
  },
  "bin": {
    "pino-stackdriver": "./index.js"
  },
  "keywords": [
    "pino",
    "pino-transport",
    "logging",
    "stackdriver"
  ],
  "author": "Osmond van Hemert <osmond@vanhemert.nu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ovhemert/pino-stackdriver.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "pino": "^5.8.0",
    "standard": "^12.0.0",
    "tap": "^12.0.1"
  },
  "dependencies": {}
}
